{
  "name": "dailang",
  "displayName": "dailang",
  "description": "dai language support",
  "version": "0.0.2",
  "publisher": "ljhua",
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yetsing/dailang-vscode-plugin.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dailang",
        "aliases": [
          "dailang",
          "dailang"
        ],
        "extensions": [
          ".dai"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dailang",
        "scopeName": "source.dailang",
        "path": "./syntaxes/dailang.tmLanguage.json"
      }
    ]
  }
}
